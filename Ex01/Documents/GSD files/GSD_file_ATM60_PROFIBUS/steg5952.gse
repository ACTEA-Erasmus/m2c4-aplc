;===============================================================================
#Profibus_DP
;
;Bitmap_Device	= "XXXXXXXn"	$ [1-7]: arbitrary(X), [8]: 'n' (compatibility)
Bitmap_Device	= "ST_AT60n"
;
;-------------------------------------------------------------------------------
; === USER Parameter definitions: -- Text / Enumeration for parameter values 
;-------------------------------------------------------------------------------
PrmText = 1
Text(0) = "Clockwise"
Text(1) = "Counter clockwise"
EndPrmText
;
PrmText = 2
Text(0) = "Disabled"
Text(1) = "Enabled"
EndPrmText
;
PrmText = 3
Text(0) = "DIP Switch"
Text(1) = "Eeprom"
EndPrmText
;-------------------------------------------------------------------------------

;-------------------------------------------------------------------------------
; === USER Parameter data definitions:	-- Name + values of parameter 
;-------------------------------------------------------------------------------
;
ExtUserPrmData = 1 "Spec_User_Prm_Byte"
Unsigned8 0 0-0
EndExtUserPrmData
;
; ------------------------------------------------------------------------------
; -- Settings of the "operating Byte"
;  0:	Direction control (CW, CCW)
;  1:	Class-2 Functionallity regarding diagnosis
;  2:	Addit. Diagnosis functionality (No meaning)
;  3:	Scaling function control (ON, OFF)
;  4_5:	reserved
;  6_7:	(Manufacturer specific)
;  6:	"SSA-Service" (Change Device-Address via telegram)
;  7:	"Address-Selection (DIP_SW, EEPROM)
;
; ------------------------------------------------------------------------------
;
ExtUserPrmData = 2 "Code sequence"
Bit(0) 0 0-1
Prm_Text_Ref = 1
EndExtUserPrmData
;
ExtUserPrmData = 3 "Class-2 functionality"
Bit(1) 1 0-1
Prm_Text_Ref = 2
EndExtUserPrmData
;
ExtUserPrmData = 4 "Diagnosis functionality"
Bit(2) 1 0-1 
Prm_Text_Ref = 2
EndExtUserPrmData
;
ExtUserPrmData = 5 "Scaling function"
Bit(3) 1 0-1
Prm_Text_Ref = 2
EndExtUserPrmData
;
ExtUserPrmData = 6 "Reserved"
BitArea (4-5) 0 0-3
EndExtUserPrmData
;
ExtUserPrmData = 7 "SSA service"
Bit(6) 0 0-1
Prm_Text_Ref = 2
EndExtUserPrmData
;
ExtUserPrmData = 8 "Address source"
Bit(7) 0 0-1
Prm_Text_Ref = 3
EndExtUserPrmData
;
; ---------------------------------------------------------------------------
ExtUserPrmData = 9 "Steps per turn --(31_16)"
Unsigned16 0 0-0
EndExtUserPrmData
;
ExtUserPrmData = 10 "Steps per turn --(15_00)"
Unsigned16 8192 1-8192
EndExtUserPrmData
;
; ------------------------------------------------------------------------------
; --- Conformity to Version OLD (Default == Max. of "Old version")
;
;  Default: [02.00].[00.00] = [10.00] * [20.00] == (12 Bit * 13 Bit)
;     Max:  [04.00].[00.00] = [20.00] * [20.00] == (13 Bit * 13 Bit)
;
ExtUserPrmData = 11 "Measuring range (steps)--(31_16)"
Unsigned16 0x0200 0-0x0400
EndExtUserPrmData
;
ExtUserPrmData = 12 "Measuring range (steps)--(15_00)"
Unsigned16 0x0000 0-0xFFFF
EndExtUserPrmData
;
