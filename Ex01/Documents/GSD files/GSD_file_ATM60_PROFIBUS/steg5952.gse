;
; === Modul Description: -- Used in Configuration telegram (1 Config Byte)
;
;	Name convention:
;	IN_xx	Configuration with xx BYTE INPUT Data
;	IO_xx	Configuration with xx BYTE INPUT Data + xx BYTE OUTPUT Data
;
;
;    -- ENCODER-Profile (Class-1, Class-2)
;    [0xD1]  Word [2], IN:  "Position Value"
;    [0xF1]  Word [2], IN:  "Position Value"
;			 Word [2], OUT: "PRESET Value"
;
;    -- Manufacturer specific
;    [0xD3]  Word [4], 	IN   (4 Byte ) "Position Value"
;    					IN   (2 Byte ) "Velocity Value"
;    					IN   (2 Byte ) "Time Stamp (ms)"
;
;    [0xF3]  Word [4], 	IN   (4 Byte ) "Position Value"
;    					IN   (2 Byte ) "Velocity Value"
;    					IN   (2 Byte ) "Time Stamp (ms)"
;    					OUT  (4 Byte ) "PRESET Value"
;
;-------------------------------------------------------------------------------
;    NOT Implemented:
;    [0xD2]  Word [3], 	{ "IN_06 (Position,Speed)" }	
;    [0xF2]  Word [3], 	{ "IO_06 (IN_06) + [4] Preset_Val " }
;
;-------------------------------------------------------------------------------
;-------------------------------------------------------------------------------
;
Module = "IN_04 (Position)" 0xD1
EndModule
;
Module = "IN_08 (Position,Speed,Time-Sig)" 0xD3
EndModule
;
Module = "IO_04 (IN_04) + PRESET-Wert " 0xF1
EndModule
;
Module = "IO_08 (IN_08) + PRESET-Wert " 0xF3
EndModule
